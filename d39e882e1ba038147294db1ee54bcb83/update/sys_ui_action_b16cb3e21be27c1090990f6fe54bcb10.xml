        <name>Select Telco and PoP PE Port</name>
        <onclick/>
        <order>300</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (!current.isActionAborted()) {
    // call a script include to perform the work
    // Instantiate the nsw2 utilities
    var nsw2util = new NcditNsw2Utils();
	var peSelectResp = nsw2util.nsw2Util_selectPoPPEPort(current);
	if (peSelectResp.status == 'OK') {
		gs.addInfoMessage("Determined PoP: " + peSelectResp.pop + ", Primary Telco: " + peSelectResp.telcoName + ", PE Port: " + peSelectResp.pePort);
	} else {
		gs.addErrorMessage("nsw2 server side script to make pe port selection was called and returned " + peSelectResp.status + " , " + peSelectResp.message);
	}
}
// redirect to here
action.setRedirectURL(current);]]></script>

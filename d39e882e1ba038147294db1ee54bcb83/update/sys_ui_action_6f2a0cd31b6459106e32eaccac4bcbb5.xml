        <list_style/>
        <messages/>
        <name>Create new location for move</name>
        <onclick/>
        <order>200</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (! current.isActionAborted()) {
	// call a script include to perform the work
	// Instantiate the nsw2 utilities
	var nsw2util = new NcditNsw2Utils();
	var createRcdsResp = nsw2util.nsw2Util_createLoc4Move(current);
	if (createRcdsResp.status == 'OK') {
		gs.addInfoMessage("nsw2 server side script created move-to location");
	} else {
		gs.addErrorMessage("nsw2 server side script to created move-to loc was called and returned " + createRcdsResp.status + " , " + createRcdsResp.message);
	}

	
	// redirect to here
    action.setRedirectURL(current);
}
]]></script>

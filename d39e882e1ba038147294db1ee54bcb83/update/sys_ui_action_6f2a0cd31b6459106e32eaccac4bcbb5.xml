    <sys_ui_action action="INSERT_OR_UPDATE">
        <action_name>nsw2_create_location</action_name>
        <active>true</active>
        <client>false</client>
        <client_script_v2><![CDATA[function onClick(g_form) {

}]]></client_script_v2>
        <comments>This will call a script include to create a new location record from request address info for a move request</comments>
        <condition>current.request_type == '2' &amp;&amp; current.provisoning_state &lt; '3'</condition>
        <form_action>true</form_action>
        <form_button>true</form_button>
        <form_button_v2>false</form_button_v2>
        <form_context_menu>true</form_context_menu>

        <name>Determine CPE required</name>
        <onclick/>
        <order>450</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (!current.isActionAborted()) {
    // call a script include to perform the work
    // Instantiate the nsw2 utilities
    var nsw2util = new NcditNsw2Utils();
    var cpeResp = nsw2util.nsw2Util_DefineReqCpe(current);
    if (cpeResp.status == 'OK') {
        gs.addInfoMessage(cpeResp.message);
    } else {
        gs.addErrorMessage("nsw2 server side script to allocate CPE was called and returned " + cpeResp.status + " , " + cpeResp.message);
    }

}
// redirect to here
action.setRedirectURL(current);]]></script>

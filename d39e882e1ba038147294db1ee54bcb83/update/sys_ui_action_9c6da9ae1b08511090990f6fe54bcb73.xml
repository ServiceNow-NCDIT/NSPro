        <name>Create related Change Req</name>
        <onclick/>
        <order>800</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (!current.isActionAborted()) {
    // call a script include to perform the work
    // Instantiate the nsw2 utilities
    var nsw2util = new NcditNsw2Utils();
    var chgResp = nsw2util.nsw2Util_createRelatedStdChangeFromPR(current);
    if (chgResp.status == 'OK') {
		gs.addInfoMessage(chgResp.message);
    } else {
        gs.addErrorMessage("nsw2 server side script to create related change request was called and returned " + chgResp.status + " , " + chgResp.message);
    }

}
// redirect to here
action.setRedirectURL(current);
]]></script>

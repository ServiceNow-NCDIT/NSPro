        <name>Verify PoP PE Port capacity</name>
        <onclick/>
        <order>280</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (!current.isActionAborted()) {
    // call a script include to perform the work
    // Instantiate the nsw2 utilities
    var nsw2util = new NcditNsw2Utils();
	var peVerifyResp = nsw2util.nsw2Util_verifyPoPPEPortCapacity(current);
	if (peVerifyResp.status == 'OK') {
		gs.addInfoMessage(peVerifyResp.message);
	} else {
		gs.addErrorMessage("nsw2 server side script to verify pe port capacity was called and returned " + peVerifyResp.status + " , " + peVerifyResp.message);
	}
}
// redirect to here
action.setRedirectURL(current);]]></script>
